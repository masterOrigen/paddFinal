    created_at timestamp with time zone null,
    constraint PlanesPublicidad_pkey primary key (id_planes_publicidad),
    constraint PlanesPublicidad_id_campania_fkey foreign key (id_campania) references "Campania" (id_campania),
    constraint PlanesPublicidad_id_cliente_fkey foreign key (id_cliente) references "Clientes" (id_cliente),
    constraint PlanesPublicidad_id_contrato_fkey foreign key (id_contrato) references "Contratos" (id),
    constraint PlanesPublicidad_id_calendar_fkey foreign key (id_calendar) references json (id_calendar),
    constraint PlanesPublicidad_id_soporte_fkey foreign key (id_soporte) references "Soportes" (id_soporte),
