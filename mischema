create table
  public.contactocliente (
    id bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    nombre character varying null,
    telefono character varying null,
    correo character varying null,
    cargo text null,
    id_cliente bigint null,
    constraint contactocliente_pkey primary key (id),
    constraint contactocliente_id_key unique (id),
    constraint contactocliente_id_cliente_fkey foreign key (id_cliente) references "Clientes" (id_cliente) on update cascade on delete cascade
  ) tablespace pg_default;

create table
  public.contactos (
    id_contacto bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    nombres character varying null,
    apellidos character varying null,
    telefono character varying null,
    email character varying null,
    id_proveedor bigint null,
    cargo text null,
    constraint contactos_pkey primary key (id_contacto),
    constraint contactos_id_proveedor_fkey foreign key (id_proveedor) references "Proveedores" (id_proveedor) on update cascade on delete cascade
  ) tablespace pg_default;

create table
  public."formatoComision" (
    "id_formatoComision" bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    "nombreFormato" character varying null,
    constraint formatoComision_pkey primary key ("id_formatoComision")
  ) tablespace pg_default;

create table
  public.json (
    id_calendar bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    id_cliente bigint null,
    "matrizCalendario" json null,
    constraint json_pkey primary key (id_calendar),
    constraint json_id_clente_fkey foreign key (id_cliente) references "Clientes" (id_cliente)
  ) tablespace pg_default;

create table
  public.plan (
    id bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    id_campania bigint null,
    nombre_plan text null,
    mes bigint null,
    anio bigint null,
    estado text null,
    estado2 text null,
    num_correlativo numeric null,
    constraint plan_pkey1 primary key (id),
    constraint plan_anio_fkey foreign key (anio) references "Anios" (id),
    constraint plan_id_campania_fkey foreign key (id_campania) references "Campania" (id_campania),
    constraint plan_mes_fkey foreign key (mes) references "Meses" ("Id")
  ) tablespace pg_default;

create table
  public.plan_alternativas (
    id bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    id_plan bigint null,
    id_alternativa bigint null,
    constraint alternativas_pkey primary key (id),
    constraint plan_alternativas_id_alternativa_fkey foreign key (id_alternativa) references alternativa (id),
    constraint plan_alternativas_id_plan_fkey foreign key (id_plan) references plan (id)
  ) tablespace pg_default;

create table
  public.programa_medios (
    id bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    id_medios bigint null,
    id_programa bigint null,
    constraint programa_medios_pkey primary key (id),
    constraint programa_medios_id_medios_fkey foreign key (id_medios) references "Medios" (id),
    constraint programa_medios_id_programa_fkey foreign key (id_programa) references "Programas" (id)
  ) tablespace pg_default;

create table
  public.programa_soporte (
    id bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    id_soporte bigint null,
    id_programa bigint null,
    constraint programa_soporte_pkey primary key (id),
    constraint programa_soporte_id_programa_fkey foreign key (id_programa) references "Programas" (id),
    constraint programa_soporte_id_soporte_fkey foreign key (id_soporte) references "Soportes" (id_soporte)
  ) tablespace pg_default;

create table
  public.proveedor_medios (
    id bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    id_medio bigint null,
    id_proveedor bigint null,
    constraint proveedor_medios_pkey primary key (id),
    constraint proveedor_medios_id_medio_fkey foreign key (id_medio) references "Medios" (id) on delete set null,
    constraint proveedor_medios_id_proveedor_fkey foreign key (id_proveedor) references "Proveedores" (id_proveedor) on delete set null
  ) tablespace pg_default;

create table
  public.proveedor_soporte (
    id bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    id_soporte bigint null,
    id_proveedor bigint null,
    constraint proveedor_soporte_pkey primary key (id),
    constraint proveedor_soporte_id_proveedor_fkey foreign key (id_proveedor) references "Proveedores" (id_proveedor) on delete set null,
    constraint proveedor_soporte_id_soporte_fkey foreign key (id_soporte) references "Soportes" (id_soporte) on delete set null
  ) tablespace pg_default;

create table
  public.soporte_medios (
    id bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    id_soporte bigint null,
    id_medio bigint null,
    constraint soporte_medios_pkey primary key (id),
    constraint soporte_medios_id_medio_fkey foreign key (id_medio) references "Medios" (id) on delete set null,
    constraint soporte_medios_id_soporte_fkey foreign key (id_soporte) references "Soportes" (id_soporte) on delete set null
  ) tablespace pg_default;