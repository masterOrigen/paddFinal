create table
  public.alternativa (
    id bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    nlinea numeric null,
    numerorden numeric null,
    anio bigint null,
    mes bigint null,
    id_campania bigint null,
    num_contrato bigint null,
    id_soporte bigint null,
    descripcion text null,
    tipo_item text null,
    id_clasificacion bigint null,
    detalle text null,
    id_tema bigint null,
    segundos text null,
    total_general numeric null,
    total_neto numeric null,
    descuento_pl numeric null,
    id_programa bigint null,
    calendar json null,
    recargo_plan numeric null,
    valor_unitario numeric null,
    medio bigint null,
    total_bruto numeric null,
    ordencreada boolean null,
    copia numeric null,
    estado_orden boolean null,
    horario_inicio time without time zone null,
    horario_fin time without time zone null,
    constraint plan_pkey primary key (id),
    constraint alternativa_id_campania_fkey foreign key (id_campania) references "Campania" (id_campania),
    constraint alternativa_id_clasificacion_fkey foreign key (id_clasificacion) references "Clasificacion" (id),
    constraint alternativa_id_programa_fkey foreign key (id_programa) references "Programas" (id),
    constraint alternativa_id_soporte_fkey foreign key (id_soporte) references "Soportes" (id_soporte),
    constraint alternativa_anio_fkey foreign key (anio) references "Anios" (id),
    constraint alternativa_medio_fkey foreign key (medio) references "Medios" (id),
    constraint alternativa_mes_fkey foreign key (mes) references "Meses" ("Id"),
    constraint alternativa_num_contrato_fkey foreign key (num_contrato) references "Contratos" (id),
    constraint alternativa_id_tema_fkey foreign key (id_tema) references "Temas" (id_tema)
  ) tablespace pg_default;

