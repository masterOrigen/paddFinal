create table
  public."Agencias" (
    id bigint generated by default as identity,
    "RazonSocial" text null,
    "NombreDeFantasia" text null,
    "NombreIdentificador" text null,
    "RutAgencia" text null,
    "Giro" text null,
    "NombreRepresentanteLegal" text null,
    "rutRepresentante" text null,
    "DireccionAgencia" text null,
    "Region" bigint null,
    "Comuna" bigint null,
    "telCelular" text null,
    "telFijo" text null,
    "Email" text null,
    estado boolean null,
    created_at timestamp with time zone null,
    codigo_megatime text null,
    c_orden boolean null,
    constraint Agencias_pkey primary key (id),
    constraint Agencias_Comuna_fkey foreign key ("Comuna") references "Comunas" (id_comuna),
    constraint Agencias_Region_fkey foreign key ("Region") references "Region" (id)
  ) tablespace pg_default;

create table
  public."Anios" (
    id bigint not null,
    years bigint null,
    constraint Anios_pkey primary key (id)
  ) tablespace pg_default;

create table
  public."Calidad" (
    id bigint not null,
    "NombreCalidad" text null,
    constraint Calidad_pkey primary key (id)
  ) tablespace pg_default;

create table
  public."Campania" (
    id_campania bigint generated by default as identity,
    "NombreCampania" text null,
    "Anio" bigint null,
    "id_Cliente" bigint null,
    "Id_Agencia" bigint null,
    "id_Producto" bigint null,
    "Presupuesto" bigint null,
    estado boolean null,
    "fechaCreacion" date null,
    c_orden boolean null,
    constraint Campania_pkey primary key (id_campania),
    constraint Campania_Anio_fkey foreign key ("Anio") references "Anios" (id),
    constraint Campania_Id_Agencia_fkey foreign key ("Id_Agencia") references "Agencias" (id) on delete set null,
    constraint Campania_id_Cliente_fkey foreign key ("id_Cliente") references "Clientes" (id_cliente),
    constraint Campania_id_Producto_fkey foreign key ("id_Producto") references "Productos" (id) on update cascade on delete cascade
  ) tablespace pg_default;

create table
  public."Clasificacion" (
    id bigint generated by default as identity,
    "NombreClasificacion" text null,
    "IdMedios" bigint null,
    id_contrato bigint null,
    constraint Clasificacion_pkey primary key (id),
    constraint Clasificacion_id_contrato_fkey foreign key (id_contrato) references "Contratos" (id)
  ) tablespace pg_default;

create table
  public."ClasificacionMedios" (
    id_clasificacion_medios bigint not null,
    "NombreClasificacion" text null,
    "Estado" boolean null,
    constraint ClasificacionMedios_pkey primary key (id_clasificacion_medios)
  ) tablespace pg_default;

create table
  public."Clientes" (
    id_cliente bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    "nombreCliente" character varying null,
    "nombreFantasia" character varying null,
    "razonSocial" character varying null,
    "id_tipoCliente" bigint null,
    grupo character varying null,
    "RUT" character varying null,
    giro text null,
    "nombreRepresentanteLegal" character varying null,
    "RUT_representante" character varying null,
    "direccionEmpresa" text null,
    "telCelular" character varying null,
    "telFijo" character varying null,
    estado boolean null,
    email character varying null,
    id_region bigint null,
    id_comuna bigint null,
    web_cliente character varying null,
    "apellidoRepresentante" character varying null,
    id_moneda bigint null,
    valor numeric null,
    id_grupo bigint null,
    id_tablaformato bigint null,
    constraint Clientes_pkey primary key (id_cliente),
    constraint Clientes_id_grupo_fkey foreign key (id_grupo) references "Grupos" (id_grupo),
    constraint Clientes_id_moneda_fkey foreign key (id_moneda) references "TipoMoneda" (id_moneda) on update cascade on delete cascade,
    constraint Clientes_id_comuna_fkey foreign key (id_comuna) references "Comunas" (id_comuna) on update cascade on delete cascade,
    constraint Clientes_id_tablaformato_fkey foreign key (id_tablaformato) references "TablaFormato" (id_tablaformato),
    constraint Clientes_id_tipoCliente_fkey foreign key ("id_tipoCliente") references "TipoCliente" ("id_tyipoCliente") on update cascade on delete cascade,
    constraint Clientes_id_region_fkey foreign key (id_region) references "Region" (id) on update cascade on delete cascade
  ) tablespace pg_default;

create table
  public."Comisiones" (
    id_comision bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    id_cliente bigint null,
    "valorComision" numeric null,
    "inicioComision" date null,
    "finComision" date null,
    tipomoneda text null,
    comision text null,
    constraint Comisiones_pkey primary key (id_comision),
    constraint Comisiones_id_cliente_fkey foreign key (id_cliente) references "Clientes" (id_cliente) on update cascade on delete cascade
  ) tablespace pg_default;

create table
  public."Comunas" (
    id_comuna bigint not null,
    "nombreComuna" text null,
    id_region bigint null,
    constraint Comunas_pkey primary key (id_comuna),
    constraint Comunas_id_region_fkey foreign key (id_region) references "Region" (id)
  ) tablespace pg_default;

create table
  public."Contratos" (
    id bigint generated by default as identity,
    "NombreContrato" text null,
    "IdCliente" bigint null,
    "IdProveedor" bigint null,
    "IdMedios" bigint null,
    "FechaInicio" date null,
    "FechaTermino" date null,
    "Estado" text null,
    "IdTipoDePublicidad" bigint null,
    "id_Mes" bigint null,
    "id_Anio" bigint null,
    "id_FormadePago" bigint null,
    "id_GeneraracionOrdenTipo" bigint null,
    "ValorNeto" text null,
    "ValorBruto" text null,
    "Descuento1" text null,
    "ValorTotal" text null,
    "Observaciones" text null,
    num_contrato bigint null,
    created_at timestamp with time zone null,
    "nombreProducto" character varying null,
    "IdAgencias" bigint null,
    c_orden boolean null,
    constraint Contratos_pkey primary key (id),
    constraint Contratos_IdCliente_fkey foreign key ("IdCliente") references "Clientes" (id_cliente) on update cascade on delete set null,
    constraint Contratos_IdMedios_fkey foreign key ("IdMedios") references "Medios" (id) on update cascade on delete cascade,
    constraint Contratos_IdProveedor_fkey foreign key ("IdProveedor") references "Proveedores" (id_proveedor) on update cascade on delete cascade,
    constraint Contratos_IdTipoDePublicidad_fkey foreign key ("IdTipoDePublicidad") references "TipoDePublicidad" ("id_Tipo_Publicidad") on update cascade on delete cascade,
    constraint Contratos_IdAgencias_fkey foreign key ("IdAgencias") references "Agencias" (id),
    constraint Contratos_id_FormadePago_fkey foreign key ("id_FormadePago") references "FormaDePago" (id),
    constraint Contratos_id_GeneraracionOrden Tipo_fkey foreign key ("id_GeneraracionOrdenTipo") references "TipoGeneracionDeOrden" (id),
    constraint Contratos_id_Mes_fkey foreign key ("id_Mes") references "Meses" ("Id") on update cascade on delete cascade,
    constraint Contratos_id_Anio_fkey foreign key ("id_Anio") references "Anios" (id) on update cascade on delete cascade
  ) tablespace pg_default;

create table
  public."Cooperado" (
    id_cooperado bigint not null,
    "NombreCooperado" text null,
    constraint Cooperado_pkey primary key (id_cooperado)
  ) tablespace pg_default;

create table
  public."Facturas" (
    id_factura bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
