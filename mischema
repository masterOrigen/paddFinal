  ) tablespace pg_default;

create table
  public."Meses" (
    "Id" bigint not null,
    "Nombre" text null,
    constraint Meses_pkey primary key ("Id")
  ) tablespace pg_default;

create table
  public."OrdenDeCompra" (
    id_orden_compra bigint generated by default as identity,
    created_at timestamp with time zone not null default now(),
    "NombreOrden" text null,
    "Codigo" text null,
    "fechaOrden" date null,
    monto bigint null,
    id_campania bigint null,
    estado boolean null,
    constraint OrdenDeComprar_pkey primary key (id_orden_compra),
    constraint OrdenDeComprar_id_campania_fkey foreign key (id_campania) references "Campania" (id_campania)
  ) tablespace pg_default;

create table
  public."OrdenesDePublicidad" (
    id_ordenes_de_comprar bigint generated by default as identity,
    "fechaCreacion" date null default now(),
    created_at timestamp with time zone null,
    id_plan bigint null,
    alternativas_plan_orden jsonb null,
    id_campania bigint null,
    copia numeric null,
    estado text null,
    usuario_registro jsonb null,
    numero_correlativo numeric null,
    orden_remplaza bigint null,
    json_remplaza jsonb null,
    id_contrato bigint null,
    id_soporte bigint null,
    constraint OrdenesDeCompra_pkey primary key (id_ordenes_de_comprar),
    constraint OrdenesDePublicidad_id_campania_fkey foreign key (id_campania) references "Campania" (id_campania),
    constraint OrdenesDePublicidad_id_contrato_fkey foreign key (id_contrato) references "Contratos" (id),
    constraint OrdenesDePublicidad_id_plan_fkey foreign key (id_plan) references plan (id),
    constraint OrdenesDePublicidad_id_soporte_fkey foreign key (id_soporte) references "Soportes" (id_soporte)
  ) tablespace pg_default;

create trigger tr_crear_orden_usuario
after insert on "OrdenesDePublicidad" for each row
execute function fn_crear_orden_usuario ();

create table
  public."OrdenesUsuarios" (
    id_orden_usuario bigint generated by default as identity,
    id_ordenes_de_comprar bigint not null,
    id_usuario bigint not null,
    fecha_asignacion timestamp with time zone null default now(),
    estado boolean null default true,
    constraint ordenesusuarios_pkey primary key (id_orden_usuario),
    constraint ordenesusuarios_id_ordenes_fkey foreign key (id_ordenes_de_comprar) references "OrdenesDePublicidad" (id_ordenes_de_comprar),
    constraint ordenesusuarios_id_usuario_fkey foreign key (id_usuario) references "Usuarios" (id_usuario)
  ) tablespace pg_default;

