    constraint Facturas_pkey primary key (id_factura),
    constraint Facturas_IdCampania_fkey foreign key ("IdCampania") references "Campania" (id_campania),
    constraint Facturas_idOrdenCompra_fkey foreign key ("idOrdenCompra") references "OrdenDeCompra" (id_orden_compra)
  ) tablespace pg_default;

